{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/abi/*": ["./abi/*"],
      "@/fhevm/*": ["./fhevm/*"],
      "@/hooks/*": ["./hooks/*"],
      "@/components/*": ["./components/*"]
    }
  },
  "include": ["./src", "./scripts", "./abi"]
}




